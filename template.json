{  
   "Resources":{  
      "CounterTable":{  
         "Type":"AWS::Serverless::SimpleTable",
         "Properties":{  
            "PrimaryKey":{  
               "Name":"id",
               "Type":"String"
            },
            "ProvisionedThroughput":{  
               "ReadCapacityUnits":5,
               "WriteCapacityUnits":5
            }
         }
      }
   },
   "Outputs":{  
      "bucket":{  
         "Value":{  
            "Fn::GetArtifactAtt":[  
               "MyAppBuild",
               "BucketName"
            ]
         }
      },
      "ObjectKey":{  
         "Value":{  
            "Fn::GetArtifactAtt":[  
               "MyAppBuild",
               "ObjectKey"
            ]
         }
      },
      "URL":{  
         "Value":{  
            "Fn::GetArtifactAtt":[  
               "MyAppBuild",
               "URL"
            ]
         }
      }
   }
}
